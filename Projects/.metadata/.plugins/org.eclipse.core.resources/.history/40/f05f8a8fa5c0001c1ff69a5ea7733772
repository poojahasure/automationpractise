package org.array;

public class Access2DArray {
	public static void main(String [] args) {
	//	int[][]arr=new int[4][4]; declaration
		
		// arr[0][1]=12
		
		//     c0  c1 c2
		// r0   1  12 13
       //  r1         5
		// r2      4
		
		int[][] arr= {{12,23,44,2},{33,44,55,3},{88,66,77,5}};// declaration + initialization
		
		for(int i=0;i<=arr.length-1;i++) {//row
			
			for(int j=0;j<arr.length;j++) {//col
				
				
				System.out.print(arr[i][j]+"  ");//arr[0][0]
			}
			
			System.out.println();
		}
		//System.out.println(arr[2][0]);
	}

}
