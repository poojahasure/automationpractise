package org.array;
// a=5 b=10       a  b  temp      temp=a
//				     10				a=b
//		 		 10	  5            b=temp
//
public class SwapArray {

	public static void main(String args[]) {
		
		int []arr= {3,2};
		int temp;
		for(int i=0;i<arr.length-1;i++) {
			
			if(arr[i]>arr[i+1]) {
				temp=arr[i+1];
				arr[i+1]=arr[i];
				arr[i]=temp;
			}
			System.out.println("{"+arr[i]+" "+arr[i+1]+"}");
		}
			
		}
	}

