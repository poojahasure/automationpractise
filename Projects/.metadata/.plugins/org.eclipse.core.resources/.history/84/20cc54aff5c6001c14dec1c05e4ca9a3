package org.oops;

class Test{
	
	private  int a=10;
	private  int b=10;
	// non static global var

	public int getA() {
		return a;
	}
   
	public void setA(int a,int b) {
		//this.a = a;
		
		//a=value
      this.a=a;
      this.b=b;
		
	}
	
}


public class EncapEx1 {
	public static void main (String []args) {
		
		Test obj= new Test();
		obj.setA(50,21);
		System.out.println(obj.getA());
		Test obj1= new Test();
		System.out.println(obj1.getA());
		

		
	}

}
