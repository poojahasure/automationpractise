package pattern;

public class Pattern {
	public static void main (String [] args) {
		Pattern.test();	
		
	}
	public static void test() {
		
		for(int i=0;i<=5;i++){ 
		for(int k=4;k>=0;k--) {
			System.out.print(" ");
		}
		for(int j=0;j<=i;j++) {
			System.out.print("* ");
		}
		System.out.println();
		}
		
	}

}
