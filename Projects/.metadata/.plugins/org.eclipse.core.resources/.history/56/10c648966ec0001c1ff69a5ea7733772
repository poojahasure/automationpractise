package org.array;

public class Access2DArray {
	public static void main(String [] args) {
		
		int[][] arr= {{12,23,44},{33,44,55},{88,66,77}};
		
		for(int i=0;i<arr.length;i++) {
			
			for(int j=0;j<arr.length;j++) {
				
				System.out.print(arr[i][j]+ "\t");
			}
			System.out.println();
		}
	}

}
