package pattern;

public class pattern123 {
	
	public static void main(String args[]) {
	
		pattern123.pattern();
	
		
	}
	public static void pattern() {
	
		for(int n=1;n<=3;n++) {
		for(int i=1;i<=n;i++) {
			//for(int j=1;j<=i;j++) {
				System.out.println(n +""+i);
	//	}
		}System.out.println();
		
	}
	}
}
