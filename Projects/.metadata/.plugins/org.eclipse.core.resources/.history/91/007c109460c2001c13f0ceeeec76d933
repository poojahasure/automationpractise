package pattern;

public class Pattern {
	public static void main (String [] args) {
		Pattern.test();	
		
	}
	public static void test() {
		
		for(int i=0;i<=4;i++){ 
		for(int k=0;k<=4;k++) {
			System.out.print(" ");
		}
		for(int j=4;j>=i;j--) {
			System.out.print("* ");
		}
		System.out.println();
		}
		
	}

}
