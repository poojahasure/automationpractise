package pattern;

public class pattern4 {

	public static void main(String args[]) {
		int n=9;

		for(int i=1;i<=9;i++) {{

			for(int j=1;j<=((9-i)-4);j++) {
				System.out.print(" ");
			}
			{
				for(int k=1;k<=i;k++) {
					System.out.print(" * ");

				}System.out.println();

			}
		}
		for(int m=1;m<=i-5;m++) {
			System.out.print(" ");
		}{
			for(int l=1;l<=10-i;l++) {
				System.out.print(" * ");
			}
			System.out.println();
		}
		}




			
}
}
