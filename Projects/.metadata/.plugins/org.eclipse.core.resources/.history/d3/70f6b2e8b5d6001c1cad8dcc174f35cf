package org.string;

public class SubString {
	public static void main (String args[]) {
   	 String s="there ist ay garden";
   	 String [] s1=s.split(" ");
//   	 String ans="";
   	 for(int i=0;i<s1.length;i++) {
   		 String firstletter=s1[i].substring(0).toUpperCase();
   		 System.out.println(firstletter);
   	 }

}
}